{
  "name": "unocss-sveltekit",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "stackblitz:dev": "node remove-links.js && npm install && vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "devDependencies": {
    "@iconify-json/logos": "^1.1.23",
    "@iconify-json/ri": "^1.1.5",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@unocss/core": "link:../../packages/core",
    "@unocss/preset-icons": "link:../../packages/preset-icons",
    "svelte": "^3.57.0",
    "svelte-check": "^3.1.4",
    "svelte-preprocess": "^5.0.2",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "unocss": "link:../../packages/unocss",
    "vite": "^4.2.0"
  },
  "stackblitz": {
    "installDependencies": false,
    "startCommand": "npm run stackblitz:dev"
  }
}
